pipeline { 
    agent none 
    stages { 
        stage('Stage-1') { 
            agent {label 'tomcat'} 
            steps { 
                sh 'touch tomcat' 
            } 
        } 
        stage('Stage-2') { 
            agent {label 'master'} 
            steps { 
                sh 'touch master' 
            } 
        } 
    } 
} 
