pipeline {
	agent {label 'java_node'} 
	stages {
		stage('Checkout') {
			steps {
				git branch: 'main', url: 'https://github.com/sudheer76R/java-example.git'
			}
		}
		stage('Static-test') {
			steps {
				echo 'Running static tests on code'
			}
		}
		stage('Build') {
			steps {
				sh 'mvn clean package'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying into Dev environment'
        echo 'rsync'
			}
		}
	}
}
