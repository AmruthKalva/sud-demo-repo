pipeline {
	agent none 
	
	stages {
		stage('ParallelJobs') {
      			agent {label 'java_node'}
			parallel {
			stage('Stage-1A'){
                             steps {
				sh 'sleep 120'
			     }
			}
			stage('Stage-1B'){
                             steps {
				sh 'sleep 120'
			     }
			}		
			}
		}
		stage('Deploy') {
    			  agent {label 'master'}
			steps {
       				echo 'sh mkdir master-1'
			}
		}
	}
}
