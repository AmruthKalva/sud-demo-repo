pipeline {
	agent {label 'java_node'}
	
	stages {
		stage('ParallelJobs') {
			parallel {
			stage('Stage-1A'){
                             steps {
				sh 'sleep 120'
			     }
			}
			stage('Stage-1B'){
                             steps {
				sh 'sleep 120'
			     }
			}		
			}
		}
		stage('Deploy') {
			steps {
       				echo 'sh mkdir master-1'
			}
		}
	}
}
